{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "jsx": "preserve",
    "allowJs": true,
    "plugins": [{ "name": "next" }],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["google.maps","node"],
    "typeRoots": ["./node_modules/@types"],
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "index.ts",
    "src",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.d.ts",
    ".next/types/**/*.ts",
    "app/pageFunctions.tsx",
    "complements/components/MapEmbed.tsx",
    "complements/components/StyleDesigner/StyleDesigner.tsx"
, "scripts/copy-css.js"  ],
  "exclude": ["node_modules"]
}
