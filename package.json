{
  "name": "nixinxwebapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "nixin": "node scripts/nixin.mjs",
    "typecheck": "tsc --noEmit",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    ".": "npm install & npm audit fix & next lint & next build & git status & git add . & git commit -m \"General Adjustments\" & git push -u origin main & cls & next dev",
    "all": "npm install & npm audit fix & next lint & next build & next postbuild & next start",
    "git": "git status & git add . & git commit -m \"General Adjustments\" & git push -u origin main & cls & next dev",
    "dev:nixinx": "dotenv -e .env.nixinx.local -- next dev -p 3001",
    "dev:elpatron": "dotenv -e .env.elpatron.local -- next dev -p 3002",
    "dev:ht-windsor": "dotenv -e .env.hottacoswindsor.local -- next dev -p 3003",
    "dev:ht-leamington": "dotenv -e .env.hottacosleamington.local -- next dev -p 3004",
    "dev:multi": "concurrently -n NIX,ELP,HTW,HTL -c auto \"pnpm:dev:nixinx\" \"pnpm:dev:elpatron\" \"pnpm:dev:ht-windsor\" \"pnpm:dev:ht-leamington\""
  },
  "dependencies": {
    "@capacitor/core": "^7.4.4",
    "@ducanh2912/next-pwa": "^10.2.4",
    "@paypal/react-paypal-js": "^8.8.3",
